{
  "name": "todo-app",
  "version": "0.0.1",
  "description": "ToDO application",
  "main": "webpack.config.js",
  "scripts": {
    "server": "node server/app.js",
    "webpack-devserver": "webpack-dev-server --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --content-base public --port 8090 --host 0.0.0.0",
    "test": "jest /test/__tests__"
  },
  "author": {
    "name": "Zharkov Ruslan",
    "email": "zharkov.ruslan.e@gmail.com"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel-cli": "^6.5.1",
    "body-parser": "^1.12.3",
    "bootstrap": "^3.3.7",
    "chai": "^4.1.2",
    "classnames": "^2.2.3",
    "cors": "*",
    "events": "^1.1.0",
    "express": "^4.15.3",
    "flux": "^2.1.1",
    "jquery": "*",
    "keymirror": "^0.1.1",
    "less": "^2.6.0",
    "mongoose": "*",
    "prop-types": "^15.5.10",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-fa": "^4.2.0",
    "react-modal-bootstrap": "^1.1.1",
    "react-prop-types": "^0.4.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "*",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-jest": "^22.0.4",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.22.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.3.0",
    "file-loader": "^0.8.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.4",
    "json-loader": "^0.5.4",
    "less-loader": "^2.2.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^1.3.0",
    "react-test-renderer": "^15.6.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
